{
  "name": "stachio",
  "version": "0.2.0",
  "description": "Render mustachio'd (Handlebars) templates into a static site.",
  "keywords": [
    "static",
    "site",
    "generator",
    "harp",
    "handlebars",
    "html",
    "layout",
    "partial",
    "template"
  ],
  "homepage": "https://github.com/jbenner-radham/node-stachio#readme",
  "bugs": {
    "url": "https://github.com/jbenner-radham/node-stachio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbenner-radham/node-stachio.git"
  },
  "license": "MIT",
  "author": "James Benner <james.benner@gmail.com> (https://www.jamesbenner.com/)",
  "main": "./lib/index.js",
  "bin": {
    "stachio": "bin/index.js"
  },
  "directories": {
    "lib": "lib",
    "man": "man",
    "example": "example",
    "test": "test"
  },
  "files": [
    "bin",
    "dist",
    "man"
  ],
  "scripts": {
    "build": "tsc",
    "make:readme": "readme-md > README.md",
    "test": "mocha --require ts-node/register --extensions ts test/spec/**/*.ts",
    "test:watch": "mocha --require ts-node/register --extensions ts --watch --watch-files test/spec/**/*.ts test/spec/**/*.ts"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.6",
    "klaw-sync": "^6.0.0",
    "lodash.negate": "^3.0.2",
    "meow": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/chai": "^4.2.15",
    "@types/fs-extra": "^9.0.6",
    "@types/klaw-sync": "^6.0.0",
    "@types/lodash.negate": "^3.0.6",
    "@types/mocha": "^8.2.1",
    "chai": "^4.3.0",
    "chalk": "^4.1.0",
    "mocha": "^10.1.0",
    "np": "^7.6.2",
    "readme-md-cli": "^0.12.0",
    "sort-package-json": "^1.57.0",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10"
  }
}
